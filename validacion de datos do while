
#include "stdafx.h"
#include "conio.h"
#include <iostream>

using namespace std;
void main()
{
	int n,p,i; //declaracion de variables n,p,i
	p=1;//validar entradas

	do {
		cout<<"ingresar numero: ";
		cin>> n;
	}while (n<0);
	for (i=1;i<=n;i++)
		p=p*i;
 cout<< "el resultado es:" <<p;
 getch();
}

